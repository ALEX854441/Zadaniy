#include <iostream>
#include <cmath>

namespace vec{
class Vector {
public:
float x, y;


float len(){
return sqrt(x*x+y*y);
};

void miror(){
x=-x;
y=-y;
};

float len2(){
return 2*sqrt(x*x+y*y);
}

Vector(float x0, float y0){
std::cout«"Конструктор"«"\n";
x=x0;
y=y0;
};

~Vector(){
std::cout«"Удаление"«"\n";
};
};
};
